#include <stdio.h>
#include <stdlib.h>
int i,j;

main()
{
      
  for(i=1; i<10; i++)
     {
       for(j=1;j<5;j++)  printf("*");
       printf("\n");    
     }
   
 system("pause");
 return 0;       
}


